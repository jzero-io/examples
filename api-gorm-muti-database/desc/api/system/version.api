syntax = "v1"

import "../base/response.api"

type Version {
	Version   string `json:"version"`
	GoVersion string `json:"goVersion"`
	Commit    string `json:"commit"`
	Date      string `json:"date"`
}

@server (
	prefix: /api/v1
	group:  system/version
)
service ntls {
	@handler GetHandler
	get /version (Empty) returns (Version)
}


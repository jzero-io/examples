syntax = "v1"

info (
	go_package:  "version"
	WrapCodeMsg: true
)

type VersionRequest {}

type VersionResponse {
	Version   string `json:"version"`
	GoVersion string `json:"goVersion"`
	Commit    string `json:"commit"`
	Date      string `json:"date"`
}

@server (
	group: version
)
service simpleapi {
	@handler Version
	get /version (VersionRequest) returns (VersionResponse)
}

